{
  "name": "high-quality-still-finder",
  "author": "HyperfocusArtisan",
  "description": "Find High Quality Images",
  "version": "0.0.6",
  "private": true,
  "main": ".output/electron/background.js",
  "scripts": {
    "dev": "nuxt dev -o",
    "build": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && electron-builder install-app-deps",
    "copy-iqa": "if not exist .output\\electron\\iqa mkdir .output\\electron\\iqa && copy .\\iqa\\iqa.exe .output\\electron\\iqa\\",
    "transpile-electron": "tsc -p ./electron --outDir .output/electron",
    "dev:electron": "NODE_ENV=development concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess 'electron ./.output/electron/background.js'\"",
    "dev:electron:win": "set NODE_ENV=development& concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess run.electron\"",
    "build:electron": "yarn build && yarn transpile-electron && yarn copy-iqa && node build.js"
  },
  "devDependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@json2csv/plainjs": "^7.0.6",
    "@nuxt/image": "^1.7.0",
    "@nuxt/ui": "^2.17.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/i18n": "^8.3.3",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@types/express": "^4.17.17",
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "electron": "^31.3.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "^3.3.3",
    "papaparse": "^5.4.1",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "tree-kill": "^1.2.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.3",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.6",
    "axios": "^1.7.2",
    "electron-log": "^5.0.0-beta.16",
    "electron-updater": "^6.3.1",
    "express": "^4.18.2",
    "papaparse": "^5.4.1"
  },
  "build": {
    "extraResources": [
      {
        "from": "iqa/iqa.exe",
        "to": "iqa/iqa.exe"
      }
    ]
  }
}
